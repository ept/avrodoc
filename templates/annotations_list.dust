<h3>Annotations</h3>
<table class="table table-striped table-hover">
    <thead>
        <tr>
            <th>Annotation</th>
            <th>Value</th>
        </tr>
    </thead>

    <tbody>
    {#.}
        <tr>
            <td class="type">{key}</td>
            {#complex}
                <td class="field-doc">{>inline_type:./}</td>
            {:else}
                <td class="field-doc">{value}</td>
            {/complex}
        </tr>
    {/.}
    </tbody>
</table>